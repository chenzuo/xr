{
  "name": "xr",
  "version": "0.2.3",
  "description": "Ultra-simple wrapper around XMLHttpRequest.",
  "main": "xr.js",
  "scripts": {
    "update-authors": "git log --format='%aN <%aE>' | sort -u | grep -v noreply > AUTHORS",
    "prepublish": "npm run build; npm run update-authors",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "babver": "babel --version",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radiosilence/xr"
  },
  "keywords": [
    "request",
    "xmlhttprequest",
    "json",
    "ajax"
  ],
  "author": "James Cleveland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radiosilence/xr/issues"
  },
  "homepage": "https://github.com/radiosilence/xr",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-react": "6.10.3",
    "webpack": "^1.12.12"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "querystring": "^0.2.0"
  }
}
